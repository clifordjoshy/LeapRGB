<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.leaprgb">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/rgb_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/rgb_icon_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.leaprgb.animationClasses.AnimationActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.leaprgb.tetrisClasses.TetrisActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.leaprgb.flooditClasses.FloodItActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.leaprgb.manualconfigClasses.ManualConfigActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.leaprgb.pongClasses.PongActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.leaprgb.snakeClasses.SnakeActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.leaprgb.MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>